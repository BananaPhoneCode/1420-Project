<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example._1420project.ViewEnrolledCoursesController"
            prefWidth="400" prefHeight="300">
    <children>
        <VBox spacing="15.0" alignment="CENTER" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="20.0" AnchorPane.rightAnchor="20.0">
            <Label text="Enrolled Courses" style="-fx-font-size: 18px; -fx-font-weight: bold;" />
            <ListView fx:id="coursesListView" prefHeight="200" />
            <Button text="Close" onAction="#handleClose" />
        </VBox>
    </children>
</AnchorPane>
